{
  "name": "zapper-cli",
  "version": "0.12.1",
  "description": "A minimal TypeScript CLI project",
  "type": "commonjs",
  "main": "dist/index.js",
  "bin": {
    "zap": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:renderer": "NODE_ENV=development tsx src/dev/rendererVibeSheet.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "bash ./etc/e2e_run.sh",
    "test:e2e:local": "npm run build && vitest run --config vitest.e2e.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "changeset publish",
    "version": "changeset version",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@eslint/js": "^9.0.0",
    "@types/commander": "^2.12.5",
    "@types/dotenv": "^8.2.3",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^16.3.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "dotenv-expand": "^12.0.3",
    "mustache": "^4.2.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.5"
  }
}
